<UserControl x:Class="CascadeStudio.NegativeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:geometry="http://gu.se/Geometry"
             xmlns:local="clr-namespace:CascadeStudio"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DataContext="{d:DesignInstance local:NegativeViewModel}"
             mc:Ignorable="d">
    <geometry:Zoombox>
        <Image x:Name="Image" Source="{Binding FileName}" />
    </geometry:Zoombox>
</UserControl>

